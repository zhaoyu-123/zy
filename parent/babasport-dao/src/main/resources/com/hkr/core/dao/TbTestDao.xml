<mapper namespace="com.hkr.core.dao.TbTestDao">
	<!-- parameterType:实体类全路径 -->
	<insert id="insertTbTest" parameterType="com.hkr.core.pojo.TbTest">
		insert into tb_test (name,birthday) values (#{name},#{birthday})
	</insert>
